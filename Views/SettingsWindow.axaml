<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:FileConvert.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450" Width="300" Height="200"
        x:Class="FileConvert.Views.SettingsWindow"
        x:DataType="vm:SettingsWindowViewModel"
        Title="SettingsWindow"
        Background="Transparent"
        TransparencyLevelHint="AcrylicBlur"
        CanResize="false"
        >
    <StackPanel>
        <Label>Settings</Label>
        <Button  Content="Save" Command="{Binding SaveSettingsSelectCommand}" HorizontalAlignment="Stretch" Margin="5, 35, 5, 5" HorizontalContentAlignment="Center"/>
    </StackPanel>
</Window>
